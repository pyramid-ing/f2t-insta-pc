{
  "name": "winsoft-insta",
  "type": "module",
  "version": "1.0.1",
  "packageManager": "pnpm@10.12.1",
  "description": "인스타그램 관련 자동 프로그램입니다.",
  "main": "dist/main/index.cjs",
  "scripts": {
    "dev": "rimraf dist && vite",
    "debug": "rimraf dist && vite -- --dsb-debug",
    "build": "rimraf dist && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "postinstall": "electron-builder install-app-deps",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset --force",
    "db:init": "prisma generate && prisma migrate deploy && npm run db:seed",
    "build-initial-db": "node scripts/build-initial-db.cjs"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.23.0",
    "@ant-design/icons": "^5.5.1",
    "@doubleshot/nest-electron": "^0.2.6",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "^11.0.9",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^11.0.9",
    "@nestjs/microservices": "^11.0.9",
    "@nestjs/platform-express": "^11.1.3",
    "@prisma/client": "^6.9.0",
    "antd": "^5.22.2",
    "axios": "^1.8.1",
    "body-parser": "^2.2.0",
    "cheerio": "^1.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dayjs": "^1.11.13",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.9",
    "electron-log": "^5.4.1",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.6.2",
    "file-type": "^19.6.0",
    "ghost-cursor": "^1.4.1",
    "google-auth-library": "^10.0.0",
    "googleapis": "^150.0.1",
    "lodash": "^4.17.21",
    "multer": "^1.4.5-lts.1",
    "nest-winston": "^1.10.2",
    "portfinder": "^1.0.37",
    "prisma": "^6.9.0",
    "puppeteer-core": "^24.10.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "react": "^18.3.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.0.1",
    "recoil": "^0.7.7",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "styled-components": "^6.1.16",
    "ts-morph": "^13.0.2",
    "user-agents": "^1.1.554",
    "winston": "^3.17.0",
    "ws": "^8.18.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@lightwing/eslint-config": "^1.0.104",
    "@types/express": "^5.0.3",
    "@types/node": "^20.19.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "@vitejs/plugin-react": "^4.5.2",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.0",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "electron": "36.4.0",
    "electron-builder": "26.0.12",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "9.29.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "events": "^3.3.0",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^9.1.7",
    "lint-staged": "16.1.2",
    "postcss": "^8.5.3",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.0.1",
    "process": "^0.11.10",
    "rimraf": "6.0.1",
    "simple-git-hooks": "2.13.0",
    "style-loader": "^4.0.0",
    "tailwindcss": "3",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.3",
    "typescript": "5.8.3",
    "vite": "6.3.5",
    "vite-plugin-doubleshot": "0.0.18",
    "wait-on": "^8.0.1",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
